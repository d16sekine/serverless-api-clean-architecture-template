{
  "permissions": {
    "allow": [
      "Bash(mkdir:*)",
      "Bash(yarn dev)",
      "Bash(yarn test:*)",
      "Bash(yarn lint:*)",
      "Bash(yarn build)",
      "Bash(git add:*)",
      "Bash(git commit:*)"
    ],
    "deny": [
      "Bash(git push:*)",
      "Bash(sudo:*)",
      "Read(.env.*)"
    ]
  },
  "includeCoAuthoredBy": false
}
